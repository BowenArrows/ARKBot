#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../async-to-gen/async-node" "$@"
  ret=$?
else 
  node  "$basedir/../async-to-gen/async-node" "$@"
  ret=$?
fi
exit $ret
